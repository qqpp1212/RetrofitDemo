<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feicuiedu.retrofitdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demoOkHttpGet.OkHttpGetActivity" />
        <activity android:name=".demoOkHttpGet2.OkHttpGet2Activity" />
        <activity android:name=".demoOkHttpPost.OkHttpPostActivity" />
        <activity android:name=".demoRetrofitGet.RetrofitActivity" />
        <activity android:name=".demoRetrofitPost.RetrofitPostActivity"></activity>
    </application>

</manifest>